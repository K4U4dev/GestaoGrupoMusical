@model IEnumerable<GestaoGrupoMusicalWeb.Models.GrupoMusicalViewModel>

@{
    ViewData["Title"] = "Grupos Musicais";
}

<div class="d-flex justify-content-between align-items-center">
    <h5>@ViewData["Title"]</h5>
    <a role="button" class="btn btn-primary" asp-controller="GrupoMusical" asp-action="Create">Novo Grupo Musical</a>
</div>

<div class="table-responsive">
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Nome)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.RazaoSocial)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Cnpj)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Cep)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Rua)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Bairro)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Cidade)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Estado)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Pais)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Email)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Youtube)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Instagram)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Facebook)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Telefone1)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Telefone2)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Banco)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Agencia)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.NumeroContaBanco)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ChavePix)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ChavePixtipo)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Nome)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.RazaoSocial)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Cnpj)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Cep)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Rua)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Bairro)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Cidade)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Estado)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Pais)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Email)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Youtube)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Instagram)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Facebook)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Telefone1)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Telefone2)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Banco)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Agencia)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.NumeroContaBanco)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ChavePix)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ChavePixtipo)
                    </td>
                    <td>
                        @Html.ActionLink("Editar", "Edit", new {  id=item.Id }) |
                        @Html.ActionLink("Detalhes", "Details", new {  id=item.Id }) |
                        @Html.ActionLink("Excluir", "Delete", new { id=item.Id })
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
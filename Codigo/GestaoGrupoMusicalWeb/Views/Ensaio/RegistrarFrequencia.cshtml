@model Core.DTO.EnsaioFrequenciaDTO
@{
    ViewData["Title"] = "Registrar Frequência";
}

<partial name="_Notificar">

<div class="container">
        <h1 class="fs-5 mb-4">@ViewData["Title"]</h1>
        <div class="row">
            <div class="col-md-4">
                <label class="control-label" asp-for="Inicio"></label>
                <input class="form-control" asp-for="Inicio" disabled />
            </div>
            <div class="col-md-4">
                <label class="control-label" asp-for="Fim"></label>
                <input class="form-control" asp-for="Fim" disabled />
            </div>
            <div class="col-md-4">
                <label class="control-label" asp-for="Tipo"></label>
                <input class="form-control" asp-for="Tipo" disabled />
            </div>
        </div>
        <partial name="_TabelaFrequencia" model="Model.Frequencias" />
</div>
